{
    "name": "@coolts/auto-openapi",
    "version": "0.0.2",
    "description": "OPEN API 3.0 - generate client API sub-package automatically",
    "main": "src/index.js",
    "repository": "https://github.com/minskiter/auto-openapi.git",
    "author": "coolts",
    "license": "MIT",
    "private": false,
    "scripts": {
        "dev": "node -r ts-node/register src/index.ts",
        "build": "tsc --build --clean"
    },
    "files": [
        "lib/**/*",
        "src/**/*"
    ],
    "keywords": [
        "OPENAPI3",
        "OPENAPI",
        "swagger",
        "axios"
    ],
    "bin": {
        "api-cli": "./lib/index.js"
    },
    "devDependencies": {
        "@types/node": "^18.11.0",
        "ts-node": "^10.9.1"
    },
    "dependencies": {
        "@types/prettier": "^2.7.1",
        "axios": "^1.1.3",
        "commander": "^9.4.1",
        "openapi3-ts": "^3.1.1",
        "prettier": "^2.7.1",
        "typescript": "^4.8.4"
    }
}
